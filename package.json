{
  "name": "@ryancui-/themis",
  "version": "1.0.1",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "type-check": "vue-tsc",
    "lint-check": "eslint \"packages/**/*.{vue,js,ts,tsx}\"",
    "test": "vitest",
    "coverage": "vitest run --coverage.enabled",
    "check": "run-p type-check lint-check coverage",
    "docs:dev": "npm run meta:update && vitepress dev packages --port 3456",
    "docs:build": "npm run meta:update && vitepress build packages",
    "meta:update": "tsx packages/metadata/scripts/update.ts",
    "build": "npm run meta:update && vite build"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist": "./dist/index.js",
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@antfu/eslint-config": "^2.3.4",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.1",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/coverage-v8": "^1.0.1",
    "@vueuse/core": "^10.7.0",
    "eslint": "^8.55.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-vue": "^9.19.2",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "jsdom": "^23.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.17.2",
    "vite": "^5.0.12",
    "vite-plugin-dts": "^3.6.4",
    "vitepress": "1.0.0-rc.31",
    "vitest": "^1.0.1",
    "vue": "^3.3.9",
    "vue-tsc": "^1.8.22"
  },
  "peerDependencies": {
    "vue": "^3"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "search-insights"
      ]
    }
  }
}
